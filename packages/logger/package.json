    "lint:js": "eslint --ext .ts,.tsx src --fix",
    "lint": "pnpm lint:js",
    "release": "pnpm publish",
    "type-check": "tsc --noEmit --pretty"
  },
  "files": ["dist"],
  "sideEffects": false
