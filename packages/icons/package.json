{
  "name": "@koobiq/react-icons",
  "version": "0.15.0",
  "description": "A collection of icons",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./manifest.json": "./manifest.json"
  },
  "type": "module",
  "keywords": [],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "directory": "packages/icons",
    "url": "git+https://github.com/koobiq/react-components.git"
  },
  "scripts": {
    "clean": "rimraf dist && rimraf node_modules && rimraf .turbo",
    "clean:temp": "rimraf temp && rimraf src",
    "lint:js": "eslint --ext .ts,.tsx src --fix",
    "lint": "pnpm lint:js",
    "type-check": "tsc --noEmit --pretty",
    "copy:svgr-to-src": "tsx scripts/svgr-copy-to-src.ts",
    "build": "vite build && tsc -p tsconfig.build.json && rm -rf dist/node_modules",
    "pull-icons": "tsx scripts/pull-icons.ts",
    "convert-svg-to-svgr": "tsx scripts/convert-svg-to-svgr.ts",
    "export-icons": "pnpm update @koobiq/icons@latest && pnpm pull-icons && pnpm convert-svg-to-svgr && pnpm lint",
    "figma:export-icons": "pnpm clean:temp && tsx pull-icons.ts && pnpm copy:svgr-to-src && pnpm lint && rimraf temp"
  },
  "devDependencies": {
    "@koobiq/icons": "^10.10.0"
  },
  "files": ["dist", "manifest.json"],
  "peerDependencies": {
    "react": "18.x || 19.x"
  },
  "sideEffects": false
}
